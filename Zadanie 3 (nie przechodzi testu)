#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);  // liczba wierszy
    if (N <= 10)
    {
    for (int i = 0; i < N; i++) {
        int val = 1;  // pierwszy element w każdym wierszu to 1
        for (int j = 0; j <= i; j++) {
            if (j == 0) {
                val = 1;  // pierwszy element zawsze 1
            } else {
                val = val * (i - j + 1) / j; // kolejne elementy
            }
            printf("%d ", val);
        }
        printf("\n"); // nowa linia po każdym wierszu
    }
    }
    else
    {
    printf("N powinno byc w przedziale od 1 do 10");
    }
    return 0;
}
